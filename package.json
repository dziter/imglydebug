    "start": "expo start --dev-client",
    "start:e2e": "E2E=true expo start -c --dev-client --port=19000",
    "maestro:ios": "maestro test --env=PLATFORM=ios e2e/maestro/",
    "maestro:android": "maestro test --env=PLATFORM=android e2e/maestro/",
    "lint": "eslint index.js app --ext .js,.ts,.tsx --max-warnings=0",
    "format": "prettier --write {.,**}/**.{ts,tsx,md,json,js}",
    "tsc": "tsc --noEmit",
    "test": "yarn lint && yarn tsc",
    "graphql:generate": "graphql-codegen --config codegen.yml",
    "pipeline": "yarn graphql:generate && yarn test && yarn jest -u",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "postinstall": "patch-package"
